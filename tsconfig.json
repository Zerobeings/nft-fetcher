{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true
  },
  "include": [
    "next-env.d.ts",
    "./src/**/*",
    "*.d.ts",
    "*.tsx",
    "*.ts"
  ],
  "exclude": ["node_modules"]
}